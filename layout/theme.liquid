    {% if canonical_url == 'https://www.simplywholesale.com.au/collections/all' %}
      <title>Order Different Products Online â€“ Simply Wholesale</title>
      <meta
        name="description"
        content="Our website has a wide range of different types of goods from many famous brands. Here, you will find something that fits you! Order today; take your chance!"
